        <animation id="linV4" span="20" type="linear">
            <controlpoint xx="0" yy="0.1" zz="0"/>
            <controlpoint xx="0" yy="-0.1" zz="0"/>
            <controlpoint xx="0" yy="0.1" zz="0"/>
            <controlpoint xx="0" yy="-0.1" zz="0"/>
            <controlpoint xx="0" yy="0.1" zz="0"/>
            <controlpoint xx="0" yy="-0.1" zz="0"/>
            <controlpoint xx="0" yy="0.1" zz="0"/>
            <controlpoint xx="0" yy="-0.1" zz="0"/>
            <controlpoint xx="0" yy="0.1" zz="0"/>
            <controlpoint xx="0" yy="-0.1" zz="0"/>
            <controlpoint xx="0" yy="0.1" zz="0"/>
